<%= form_with(model: @supplier) do |f| %>
  <div>
    <%= f.label :corporate_name, 'Razão social' %>
    <%= f.text_field :corporate_name %>
  </div>
  <div>
    <%= f.label :brand_name, 'Nome fantasia' %>
    <%= f.text_field :brand_name%>
  </div>
  <div>
    <%= f.label :registration_number, 'CNPJ' %>
    <%= f.text_field :registration_number %>
  </div>
  <div>
    <%= f.label :city, 'Cidade' %>
    <%= f.text_field :city %>
  </div>
  <div>
    <%= f.label :state, 'Estado' %>
    <%= f.text_field :state %>
  </div>
  <div>
    <%= f.label :full_address, 'Endereço' %>
    <%= f.text_field :full_address %>
  </div>
  <div>
    <%= f.label :phone, 'Telefone' %>
    <%= f.text_field :phone %>
  </div>
  <div>
    <%= f.label :email, 'Email' %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.submit 'Cadastrar' %>
  </div>
<% end %>