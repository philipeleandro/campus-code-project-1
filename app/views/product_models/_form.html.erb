<%= form_with(model: @product_model) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :weigth %>
    <%= f.number_field :weigth%>
  </div>
  <div>
    <%= f.label :width %>
    <%= f.number_field :width %>
  </div>
    <div>
    <%= f.label :height %>
    <%= f.number_field :height %>
  </div>
  <div>
    <%= f.label :depth %>
    <%= f.number_field :depth %>
  </div>
  <div>
    <%= f.label :sku %>
    <%= f.text_field :sku %>
  </div>
    <div>
    <%= f.label :supplier_id %>
    <%= f.collection_select :supplier_id, Supplier.order(:brand_name), :id, :brand_name %>
  </div>
  <div>
    <%= f.submit 'Cadastrar' %>
  </div>
<% end %>